{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"moduleResolution": "bundler",
		"target": "ESNext",
		"module": "esnext",
		"strict": true,
		"verbatimModuleSyntax": false,
		"skipLibCheck": true,
	},
	"exclude": [
		"node_modules"
	]
}